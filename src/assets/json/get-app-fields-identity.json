{
	"model": [],
	"pageflow": [ { "prevPage": "", "path": "get-started", "nextPage": "personal-info" }, { "prevPage": "identity", "path": "personal-info", "nextPage": "identity" }, { "prevPage": "personal-info", "path": "identity", "nextPage": null } ],
	"fields": [
		{
			"template": "<h6 class=\"pull-left\">Please select your identification type:</h6>",
			"data": {
				"sectionType": "IP-IdentityType_PA"
			}
		},
		{
			"wrappers": "section",
			"fieldGroupClassName": "row",
			"fieldGroup": [
				{
					"key": "Identity_Information__c.ID_Type__c",
					"type": "select",
					"className": "col-sm-6 col-md-4 col-lg-6 pull-right",
					"templateOptions": {
						"label": "ID Type",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "ID_Type__c"
					},
					"defaultValue": "Drivers License",
					"data": {
						"fieldsToReset": [ "Issue_Date__c", "Identity_Number_Primary__c", "State_Issued__c", "Expiry_Date__c", "Country_Issued__c" ]
					}
				}
			]
		},
		{
			"template": "<h5 class=\"input-group-title\">Identity Information</h5>",
			"className": "col-xs-12 col-sm-12 col-md-12 col-lg-12 inline-section-header",
			"data": {
				"sectionType": "IP-IdentityInfo_PA"
			}
		},
		{
			"wrappers": "section",
			"fieldGroupClassName": "row",
			"fieldGroup": [
				{
					"key": "Identity_Information__c.Identity_Number_Primary__c",
					"type": "input",
					"className": "col-xs-12 col-sm-9 col-md-6 col-lg-6",
					"templateOptions": {
						"label": "Identification Number",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "Identity_Number_Primary__c"
					}
				},
				{
					"key": "Identity_Information__c.State_Issued__c",
					"type": "select",
					"className": "col-xs-12 col-sm-9 col-md-6 col-lg-6",
					"templateOptions": {
						"label": "State Issued",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "State_Issued__c"
					},
					"expressionProperties": {
						"templateOptions.required": "model['Identity_Information__c']['ID_Type__c'] !='Military ID'"
					},
					"hideExpression": "model['Identity_Information__c']['ID_Type__c'] == 'Passport' || model['Identity_Information__c']['ID_Type__c'] == 'Military ID'"
				},
				{
					"key": "Identity_Information__c.Country_Issued__c",
					"type": "select",
					"className": "col-xs-12 col-sm-9 col-md-6 col-lg-6",
					"templateOptions": {
						"label": "Country Issued",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "Country_Issued__c"
					},
					"hideExpression": "model['Identity_Information__c']['ID_Type__c'] != 'Passport'"
				},
				{
					"key": "Identity_Information__c.Issue_Date__c",
					"type": "date",
					"className": "col-xs-12 col-sm-9 col-md-6 col-lg-6",
					"templateOptions": {
						"label": "Issue Date (MM/DD/YYYY)",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "Issue_Date__c"
					}
				},
				{
					"key": "Identity_Information__c.Expiry_Date__c",
					"type": "future_date",
					"className": "col-xs-12 col-sm-9 col-md-6 col-lg-6",
					"templateOptions": {
						"label": "Expiration Date (MM/DD/YYYY)",
						"required": true,
						"objectName": "Identity_Information__c",
						"fieldName": "Expiry_Date__c"
					}
				}
			]
		}
	]
}
